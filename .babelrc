{
  "presets": [
    "react-native"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "routes": "./src/routes",
          "components": "./src/components",
          'config' : "./src/configs",
          'utils' : "./src/utils",
        },
      }
    ],
    [
      "babel-plugin-root-import"
    ]
  ]
}